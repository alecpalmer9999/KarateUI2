<script language="JavaScript" type="text/javascript">
    var iChanged;
    var portionUsed = "NOTSELECTED";
    $(document).ready(function () {
        var editjbt = "no";
        if ((document.getElementsByName('editJobTicket')[0] != null)
            && (document.getElementsByName('editJobTicket')[0] != 'undefined')) {
            if (document.TheForm.editJobTicket.value == "yes")
                editjbt = "yes";
        }

        if (editjbt == "yes") {
            $('#offerIDValue').attr("disabled", true);
            $('#offerIDValue').css('background-color', "#dddddd");
        } else {
            $('#offerIDValue').attr("disabled", false);
            $('#offerIDValue').css('background-color', "");
        }
        if ($('input[name=isStartPage]').val() === 'true') {
            gtag('event', 'Link from content', {
                'event_category': 'Welcome to RightsLink'
            });
        }
    });

    onbeforeunload = function () {
        if (iChanged && iChanged.selectedIndex != 0)
            iChanged.selectedIndex = -1;
    };

    function switchOffer(rentalOnly, purchaseOnly, offerIDValue) {
        if (document.TheForm.rentalOnly) {
            document.TheForm.rentalOnly.value = rentalOnly;
        }
        if (document.TheForm.purchaseOnly) {
            document.TheForm.purchaseOnly.value = purchaseOnly;
        }
        if ((document.TheForm.offerIDValue) && (offerIDValue > -1)) {
            document.TheForm.offerIDValue.value = offerIDValue;
        }
        offerChange(document.TheForm.offerIDValue, 'quickprice');
    }

    function placeOrder() {
        gtag('event', 'Continue', {
            'event_category': 'Quick Price Estimate',
            'event_label': $("#priceDouble").first().text()
        });
        if (document.TheForm.loggedIn.value == "true") {
            document.TheForm.doCalc.value = "true";
            document.TheForm.targetPage.value = "additionaldatapermissions";
        } else {
            document.TheForm.passThroughTarget.value = "additionaldatapermissions";
            document.TheForm.targetPage.value = "inflowlogin";
        }
        document.TheForm.ignoreErrors.value = "false";
        document.TheForm.isMyAccount.value = "false";
        document.TheForm.target = "_top";
        document.TheForm.buttonClicked.value = "continue";
        document.TheForm.submit();
    }

    function overrideCurrentTargetPage(targetValue) {
        document.TheForm.currentTargetPage.value = targetValue;
    }

    function checkLen(textbox, maxlimit) {
        if (textbox.value.length > maxlimit) {
            textbox.value = textbox.value.substring(0, maxlimit);
        }
    }

    function submitNoCalc(permissionChoice, targetValue, ignoreTheErrors) {
        document.TheForm.doCalc.value = "false";
        document.TheForm.targetPage.value = targetValue;
        if (ignoreTheErrors == "true")
            document.TheForm.ignoreErrors.value = "true";
        else
            document.TheForm.ignoreErrors.value = "false";
        document.TheForm.passThroughTarget.value = "";
        document.TheForm.isMyAccount.value = "false";
        document.TheForm.target = "_top";
        document.TheForm.submit();
    }

    function offerChange(permissionChoice, targetValue, grantDenyCheckIgnoreErrors) {
        switch(permissionChoice) {
            case document.TheForm.offerIDValue:
                gtag('event','Select TOU', {
                    'event_category': 'Welcome to RightsLink',
                    'event_label': 'Select TOU'
                });
                clearCheckBoxesInSessionStorage();
                break;
            // RL-31777: Med Comms: 'Number of portions' / 'translations' does not clear after change Portion / Translation (RL-31770)
            case document.TheForm.portionUsed:
                resetInputFieldOnDropDownChange("numPortions");
                break;
            case document.TheForm.translating:
                resetInputFieldOnDropDownChange("numLanguages");
        }
        document.TheForm.doCalc.value = "false";
        document.TheForm.targetPage.value = targetValue;
        document.TheForm.ignoreErrors.value = "false";
        document.TheForm.passThroughTarget.value = "";	//TM, 11-19-2002 reset to avoid incoherent orders
        document.TheForm.isMyAccount.value = "false";
        document.TheForm.target = "_top";
        iChanged = permissionChoice;
        if (!iChanged && permissionChoice == document.TheForm.RequesterType) {
            if (document.TheForm.requesterType)
                iChanged = document.TheForm.requesterType;
            else if (document.TheForm.userType)
                iChanged = document.TheForm.userType;

        }
        if ((document.TheForm.grantDenyCheckIgnoreErrors) && (grantDenyCheckIgnoreErrors)) {
            document.TheForm.grantDenyCheckIgnoreErrors.value = grantDenyCheckIgnoreErrors;
        }
        document.TheForm.action = document.TheForm.action + "#formTop"
        document.TheForm.submit();
    }

    function intendedUseChange(targetValue) {
        document.TheForm.doCalc.value = "false";
        document.TheForm.targetPage.value = targetValue;
        document.TheForm.ignoreErrors.value = "false";
        document.TheForm.passThroughTarget.value = "";
        document.TheForm.isMyAccount.value = "false";
        document.TheForm.target = "_top";
        document.TheForm.action = document.TheForm.action + "#formTop"
        document.TheForm.submit();
    }

    function portionChange(permissionChoice, targetValue) {
        document.TheForm.doCalc.value = "false";
        document.TheForm.targetPage.value = targetValue;
        document.TheForm.ignoreErrors.value = "false";
        document.TheForm.passThroughTarget.value = "";
        document.TheForm.isMyAccount.value = "false";
        document.TheForm.target = "_top";
        iChanged = permissionChoice;
        document.TheForm.action = document.TheForm.action + "#formTop"
        document.TheForm.submit();
    }

    function openCCCLink(link, windowName) {
        window.open(link, windowName);
        return false;
    }

    function quickPrice(permissionChoice, targetValue, doCalc) {
        gtag('event', 'Get quick price', {
            'event_category': 'Quick Price Estimate',
            'event_label': $("#priceDouble").first().text()
        });
        if (targetValue === 'moreoptions') {
            if (doCalc) {
                document.TheForm.doCalc.value = "true";
                document.TheForm.ignoreErrors.value = "false";
            } else {
                document.TheForm.doCalc.value = "false";
                document.TheForm.ignoreErrors.value = "true";
            }
        } else {
            document.TheForm.doCalc.value = "true";
            document.TheForm.ignoreErrors.value = "false";
        }
        document.TheForm.targetPage.value = targetValue;
        document.TheForm.isMyAccount.value = "false";
        document.TheForm.target = "_top";
        document.TheForm.buttonClicked.value = "quickprice";
        document.TheForm.action = document.TheForm.action + "#formTop"
        document.TheForm.submit();
    }

    function showTerms(thePublisher) {
        var location = "/App/SpecialTerms.jsp?termsSource=Content";
        window.open(location, '_Terms', 'location=no,toolbar=no,status=no,width=500,height=400,scrollbars=yes,resizable=yes');
    }

    function quickBuyTerms() {
        document.TheForm.buttonClicked.value = "quickbuy";
        if (document.TheForm.quickBuy)
            document.TheForm.quickBuy.value = "true";

        if (document.TheForm.termsAgree) {
            if (document.TheForm.termsAgree.checked) {
                finishOrderTerms();
            } else {
                window.alert("Please check the \"I agree to these terms and conditions\" check box.");
            }
        } else {
            finishOrderTerms();
        }
    }

    function continueTerms() {
        document.TheForm.buttonClicked.value = "payment";
        if (document.TheForm.quickBuy)
            document.TheForm.quickBuy.value = "false";

        if (document.TheForm.termsAgree) {
            if (document.TheForm.termsAgree.checked) {
                finishOrderTerms();
            } else {
                window.alert("Please check the \"I agree to these terms and conditions\" check box.");
            }
        } else {
            finishOrderTerms();
        }
    }

    function finishOrderTerms() {
        if (document.TheForm.loggedIn.value == "true") {
            document.TheForm.targetPage.value = "payment-method-select";
            document.TheForm.passThroughTarget.value = "createjobticket";
            if (document.TheForm.buttonClicked.value == "payment") {
                document.TheForm.buttonClicked.value = "";
            }
        } else {
            document.TheForm.targetPage.value = "inflowlogin";
            document.TheForm.passThroughTarget.value = "payment-method-select";
        }
        document.TheForm.ignoreErrors.value = "false";
        document.TheForm.target = "_top";
        document.TheForm.isMyAccount.value = "false";
        document.TheForm.buttonClicked.value = "accept";
        document.TheForm.submit();
    }

    function placeOrderTerms(targetPage) {
        if (document.TheForm.termsAgree.checked) {
            if (document.TheForm.loggedIn.value == "true") {
                document.TheForm.targetPage.value = targetPage;
            } else {
                document.TheForm.targetPage.value = "inflowlogin";
                document.TheForm.passThroughTarget.value = targetPage;
            }
            document.TheForm.ignoreErrors.value = "false";
            document.TheForm.target = "_top";
            document.TheForm.isMyAccount.value = "false";
            document.TheForm.buttonClicked.value = "accept";
            document.TheForm.submit();
        } else {
            window.alert("Please check the \"I agree to these terms and conditions\" check box.");
        }
    }

    function clicked(checkbox, correHiddenName) {
        var correHidden = document.getElementsByName(correHiddenName)[0];
        if (checkbox.checked)
            correHidden.value = "checked";
        else
            correHidden.value = "";
    }

    function disclaimerUpdateClicked() {
        var disclaimerCheckBox = document.getElementsByName('disclaimerLanguageSelected')[0];
        if (disclaimerCheckBox.checked) {
            document.TheForm.targetPage.value = "additionaldata";
        } else {
            document.TheForm.targetPage.value = "confirmorder";
        }
    }

    function djWRROfferChange(permissionChoice, targetValue) {
        if (permissionChoice === document.TheForm.offerIDValue) {
            clearCheckBoxesInSessionStorage();
        }
        document.TheForm.targetPage.value = targetValue;
        document.TheForm.doCalc.value = "false";
        document.TheForm.ignoreErrors.value = "false";
        document.TheForm.passThroughTarget.value = "";	//TM, 11-19-2002 reset to avoid incoherent orders
        document.TheForm.isMyAccount.value = "false";
        document.TheForm.target = "_top";
        document.TheForm.submit();
    }

    function djWRExpiredGoHome() {
        if (confirmBeforeProceed === true) {
            showConfirm("home");
            return;
        }

        $('img[name=home]').parents('a').removeAttr('href'); // to click once
        if (document.TheForm) {

            if (document.TheForm.doCalc) {
                document.TheForm.doCalc.value = "false";
            }
            if (document.TheForm.orderBeanReset) {
                document.TheForm.orderBeanReset.value = "true";
            }
            if (document.TheForm.targetPage) {
                document.TheForm.targetPage.value = "";
            }
            if (document.TheForm.passThroughTarget) {
                document.TheForm.passThroughTarget.value = "";
            }
            if (document.TheForm.ignoreErrors) {
                document.TheForm.ignoreErrors.value = "true";
            }
            document.TheForm.target = "_top";
            if (document.TheForm.isMyAccount) {
                document.TheForm.isMyAccount.value = "false";
            }
            if (document.TheForm.buttonClicked) {
                document.TheForm.buttonClicked.value = "home";
            }

            if ((document.TheForm.currentTargetPage.value === "webreprintexpired") && (document.TheForm.djSROorder != null) && (document.TheForm.djSROorder.value === "true")) {
                document.TheForm.targetPage.value = "quickpriceWRA";
            }

            document.TheForm.target = "_top";
            document.TheForm.action = "/AppDispatchServlet";

            clearTerms();
            document.TheForm.submit();
        }
    }

    function resetInputFieldOnDropDownChange(inputField) {
        if(document.getElementById(inputField) != null) {
            document.getElementById(inputField).value = "";
        }
    }
</script>
<script language="JavaScript" type="text/javascript">
    // reset the form upon entry.  This is provided to protect the form from being
    // out of synch.  Because of the onChange events and the dreaded back button
    // it is possible for the offer to be out of synch with the remaining form data.
    // The reset of the form should nullify that possibility.
    document.TheForm.reset();
    setDefaultCookie("OrderThreadID", "-1718958182189934204", false);
</script>





<script language="JavaScript" type="text/javascript">

    var el = document.TheForm.elements['keyPressTarget'];
    
    var isLogin = el && el.value == 'authenticate';
    
    var isBack = getCookie("history_back_event") == '1';
    removeCookie("history_back_event");

    
    if (isBack) {
        
        el = document.TheForm.elements['currentTargetPage'];
        if (el && el.value.indexOf('createaccount') != -1) {
            goQuickPrice();
        }
    } else if (isLogin) {
        doCasLogin();
    }
    

    
    history.back = function () {
        setDefaultCookie("history_back_event", "1");
        history.go(-1);
    };

    if (typeof jQuery === "undefined") {
        var script_tag = document.createElement('script');
        script_tag.setAttribute("type", "text/javascript");
        script_tag.setAttribute("src", "/resources/jquery/latest/jquery-1.11.3.js");
        script_tag.onload = main;
        script_tag.onreadystatechange = function () {
            if (this.readyState == 'complete' || this.readyState == 'loaded') main();
        }
        document.getElementsByTagName("head")[0].appendChild(script_tag);
    } else {
        main();
    }

    
    $(window).bind('beforeunload', function () {
        new Image().src = "/CAS/Images/singlepixel.cas?" + new Date().getTime();
        saveQuickPriceEstimateCheckBoxState();
    });

    function main() {
        var pageid;
        if (document.TheForm.currentTargetPageID) {
            pageid = document.TheForm.currentTargetPageID.value;
        } else {
            pageid = -1;
        }
        var linktext = $("#debuglink").text();
        linktext += " : " + pageid;
        $("#debuglink").prop("title", pageid);
        $("#debuglink").html(linktext);
    }

    function paymentTerms() {
        var location = "/App/PaymentTermsAndConditions.jsp";
        window.open(location, '_Terms', 'location=no,toolbar=no,status=no,width=500,height=400,scrollbars=yes,resizable=yes');
    }
</script>

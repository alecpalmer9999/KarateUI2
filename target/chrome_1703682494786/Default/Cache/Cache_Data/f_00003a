    <script type="text/javascript">
function openHelp(page)
{
	var loadedWindow = false;
	if (page)
	{
        // % of screen resolution
        let popupScreenSize = 0.65;

        let customizedWidth = screen.width * popupScreenSize;
        let customizedHeight = screen.height * popupScreenSize;
		rHelp = window.open('', 'rHelp', 'menubar=yes,toolbar=yes,location=no,status=no,width=' + customizedWidth + ',height=' + customizedHeight + ',scrollbars=yes,resizable=yes');
		if (rHelp.frames.length == 2)
		{
			if (rHelp.frames[1].frames.length == 2)
			{
				theWindow = rHelp.frames[1].frames[1];
				theWindow.document.location= page;
				theWindow.focus();
				loadedWindow = true;
			}
		}
		if (!loadedWindow)
		{
			
			FullPopUp(page, 'rHelp');
		}
	}
	else
	{
	    FullPopUp('/help/rightslinkhelppages/helpstart.htm', 'rHelp');
	}
}

function openTC()
{
	var location = '/clients/ccc_creditcard_tc.html';
	window.open(location, '_Terms', 'location=no,toolbar=no,status=no,width=500,height=400,scrollbars=yes,resizable=yes');

}
function openPubFAQ(anchor){
            var urlToUse = "/help/rightslinkhelppages/Frequently_Asked_Questions_";
            var pubContent = "ELS";
            var pubOrder = "ELS";
            var pubAttr = "ELS";
            if (pubContent){
                urlToUse += pubContent.toLowerCase().replace(" ", "_") + ".htm";
            } else if(pubOrder){
                urlToUse += pubOrder.toLowerCase().replace(" ", "_") + ".htm";
            } else if(pubAttr){
                urlToUse += pubAttr.toLowerCase().replace(" ", "_") + ".htm";
            } else {
                urlToUse = "/help/rightslinkhelppages/welcome_to_rightslink.htm";
            }
            if (anchor){
                urlToUse = urlToUse + "#" + anchor;
            }
            FullPopUp(urlToUse, 'rHelp');
    }

function openRFPHelp(name, title) {
    if (name) {
        title = name.startsWith('publisherStmSignatory.') ? "Publisher, STM Signatory" : title;
        FullHelpPopUp('/App/RFPHelp.jsp?name=' + name, 'rOpenHelp', title);
    }
}

function metaDataLogin()
{
    document.TheForm.doCalc.value = "false";
	document.TheForm.targetPage.value= "metadatalogin";
    document.TheForm.passThroughTarget.value = "additionaldatapermissions";
	document.TheForm.ignoreErrors.value= "true";
	document.TheForm.target = "_top";
	document.TheForm.isMyAccount.value = "false";
	document.TheForm.buttonClicked.value = "logina";
	document.TheForm.submit();
}


function resetPassword()
{
    document.TheForm.targetPage.value = "resetPassword";
    document.TheForm.passThroughTarget.value = "resetPasswordStatus";
    document.TheForm.ignoreErrors.value= "false";
    document.TheForm.buttonClicked.value = "continue";
    document.TheForm.submit();
}


function outOfUS()
{

    openHelp('/help/rightslinkhelppages/Shipping_&_Delivery.htm#International_Shipping');

}


function autoLoginHelp()
{
    openHelp('/help/rightslinkhelppages/Enable_Auto_Log-in.htm');
}

function taxHelp()
{

    openHelp('/help/rightslinkhelppages/Payment_Pricing_Promotions.htm#Tax');

}


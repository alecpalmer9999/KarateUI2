<script language="JavaScript" type="text/javascript">
    // trick to display QuickPrice page in TwoColumn format
    $(window).on('load', function () {
        splitIntoTwoColumns(".quick-price-inner-block");
        updateErrorChangeEvent();
        // saved checkboxes state on page reload
        // If buttons 'Copy Order', 'Edit Order', 'Resubmit Order' in MyAccount,
        // 'copy order', 'edit order' in RLAdmin clicked, checkboxes shouldn't be updated from session storage
        let isCopyOrder = location.search.split('copyOrder=')[1] && location.search.split('copyOrder=')[1].startsWith("Yes");
        let isEditJobTicket = location.search.split('editJobTicket=')[1] && (location.search.split('editJobTicket=')[1].startsWith("yes"));
        if (!(isCopyOrder || isEditJobTicket || (location.search.split('originalOrderUID=')[1]))) {
            originalOrderUID = updateQuickPriceEstimateCheckBoxState();
        }
        if (isEditJobTicket) {
            gtag('event', 'Edit Job Ticket', {
                'event_category': 'Quick Price Estimate'
            });
        }
        if (isCopyOrder) {
            gtag('event', 'Copy Order', {
                'event_category': 'Quick Price Estimate'
            });
        }
    });
</script>


